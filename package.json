{
  "name": "monitor-sdk",
  "version": "1.0.0",
  "description": "前端监控 sdk",
  "author": "xiaolin",
  "main": "dist/monitor.js",
  "module": "dist/monitor.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "scripts": {
    "watch": "rollup -w -c rollup/dev.js",
    "serve": "nodemon ./server/app.js",
    "build": "rollup -c rollup/build.js",
    "prepare": "husky install",
    "commit": "lint-staged",
    "fix": "eslint packages/**/*.*  --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run commit",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/*.{ts,js}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/node": "^16.4.10",
    "error-stack-parser": "^2.1.4",
    "jest": "^27.1.0",
    "json-stringify-safe": "^5.0.1",
    "lerna": "^3.22.1",
    "lint-staged": "^11.1.1",
    "rollup": "^2.55.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-uglify": "^6.0.4",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "babel-plugin-no-debugging": "^1.3.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-es2015-rollup": "^3.0.0",
    "core-js": "^3.24.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.1",
    "koa2-cors": "^2.0.6",
    "lint": "^0.8.19",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^3.0.3",
    "puppeteer": "^19.8.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "rollup-watch": "^4.3.1"
  },
  "sideEffects": false
}
